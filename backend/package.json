{
  "name": "peer2park",
  "version": "1.0.0",
  "description": "Peer2Park API to store/fetch parking spots",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json && sam build",
    "deploy": "sam deploy --guided",
    "clean": "rimraf ./dist .aws-sam",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Peer2Park/Peer2Park.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Peer2Park/Peer2Park/issues"
  },
  "homepage": "https://github.com/Peer2Park/Peer2Park#readme",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.921.0",
    "@aws-sdk/client-dynamodb": "^3.907.0",
    "@aws-sdk/client-s3": "^3.907.0",
    "@aws-sdk/lib-dynamodb": "^3.907.0",
    "@types/aws-lambda": "^8.10.155"
  },
  "devDependencies": {
    "@types/node": "^24.7.1",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
